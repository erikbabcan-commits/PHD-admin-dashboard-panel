
{
  "version": 2,
  "builds": [
    {
      "src": "index.tsx",
      "use": "@vercel/angular@2",
      "config": {
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "API_KEY": "@api_key_gemini"
  }
}